{
  "manifest_version": 2,

  "name": "Seen It",
  "description": "Skip over Imgur images that you've already seen.",
  "version": "2.0.0",
  "browser_action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "19": "images/icon-19.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "default_title": "Seen It",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
